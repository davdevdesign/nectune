{
  "name": "nectune",
  "description": "Nectune push entertainment directly on your Terminal",
  "version": "0.0.3",
  "author": "David Guenin @davdevdesign",
  "bin": {
    "nectune": "./bin/run"
  },
  "bugs": "https://github.com/davdevdesign/nectune/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^2",
    "chalk": "^4.1.0",
    "columnify": "^1.5.4",
    "node-emoji": "^1.10.0",
    "node-fetch": "^2.6.1",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10.17.31",
    "globby": "^10",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/davdevdesign/nectune",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "nectune",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "helpClass": "./lib/help",
    "macos": {
      "identifier": "com.nectune.cli"
    }
  },
  "repository": "davdevdesign/nectune",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
